{
  "name": "hl7-viewer",
  "version": "1.0.0",
  "scripts": {
    "format": "prettier --write src"
  },
  "private": true,
  "devDependencies": {
    "husky": "~4.2.5",
    "lint-staged": "~11.0.0",
    "prettier": "~2.3.0"
  },
  "lint-staged": {
    "scripts/*.js": [
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
